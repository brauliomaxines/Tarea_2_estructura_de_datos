      "source": [
        "def Fibonacci( n ):\n",
        "    if n < 2:\n",
        "        return n\n",
        "    else:\n",
        "        return Fibonacci( n - 1 ) + Fibonacci ( n - 2 )\n",
        "\n",
        "for x in range(10):\n",
        "        print( Fibonacci(x) )"
      ],
